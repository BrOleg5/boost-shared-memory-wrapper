# Config file for the SharedMemory package
# It defines the following variables
#  SharedMemory_INCLUDE_DIRS - include directories for SharedMemory
#  SharedMemory_LIBRARIES    - libraries to link against
#  SharedMemory_EXECUTABLE   - the tests executable

# Compute paths
get_filename_component(SHAREDMEMORY_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(SharedMemory_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET SharedLibraryLib AND NOT SharedMemory_BINARY_DIR)
    include("${SHAREDMEMORY_CMAKE_DIR}/SharedMemoryTargets.cmake")
endif()

# These are IMPORTED targets created by SharedMemoryTargets.cmake
set(SharedMemory_LIBRARIES SharedMemoryLib)
if(@INSTALL_TESTS@)
    set(SharedMemory_EXECUTABLE transmitter receiver)
endif()